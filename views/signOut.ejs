<%- include("templates/header") %>
<form action="/signOut" method="post" class="grid bg-purple-400 w-1/2 mx-auto p-2 rounded-2xl">
    <label for="password" class="text-center">Enter your password to confirm account deletion</label>
    <input type="password" id="password" name="password" class="rounded-2xl px-2 focus:outline-none" required>
    <button type="submit" class="bg-orange-400 border-2 border-orange-500 mt-2 px-2 rounded-3xl">Delete Account</button>
</form>
